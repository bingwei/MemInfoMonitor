<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:orientation="vertical">
     
     <LinearLayout
	     android:layout_width="match_parent"
	     android:layout_height="wrap_content"
	     android:gravity="center_vertical"
		 android:orientation="vertical">
		
		 <!-- process name -->
		<TextView android:id="@+id/service_process_name"
			android:textSize="14dip"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="service_process_name"
			android:layout_marginTop="5sp"
		/>
		
	    <!-- service name -->
		<TextView android:id="@+id/service_service_name"
			android:textSize="14dip"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="service_service_name"
		/>
		     
			
		<!-- started-->
		<TextView android:id="@+id/service_started_status"
			android:textSize="14dip"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="service_started_status"
		/>
			
	 </LinearLayout>
 </LinearLayout>
